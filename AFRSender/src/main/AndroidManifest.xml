<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.basetis.afrsender.afrsender" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.basetis.afrsender.afrsender.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".MainBroadcastReceiver" android:enabled="true">
            <intent-filter>
                <action android:name="COM.BASETIS.BLINKING.BROADCAST.CALLBACK.AFR"></action>
            </intent-filter>
        </receiver>
    </application>

</manifest>
